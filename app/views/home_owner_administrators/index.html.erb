<p id="notice"><%= notice %></p>

<%= link_to 'New Home owner administrator', new_home_owner_administrator_path, class: "btn btn-primary" %>
<br />
<br />
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th colspan="3">Options</th>
    </tr>
  </thead>

  <tbody>
    <% @home_owner_administrators.each do |home_owner_administrator| %>
      <tr>
        <td><%= home_owner_administrator.name %></td>
        <td><%= home_owner_administrator.email %></td>
        <td><%= home_owner_administrator.mobile %></td>
        <td><%= link_to 'Show', home_owner_administrator, class: "btn btn-default btn-sm" %></td>
        <td><%= link_to 'Edit', edit_home_owner_administrator_path(home_owner_administrator) , class: "btn btn-default btn-sm"%></td>
        <td><%= button_to 'Destroy', home_owner_administrator, method: :delete, data: { confirm: 'Are you sure?' },class: "btn btn-danger btn-sm" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


