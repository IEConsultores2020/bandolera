<p id="notice"><%= notice %></p>

<h1>Listing Tenants</h1>

<table>
  <thead>
    <tr>
      <th>Property</th>
      <th>Monthly rent</th>
      <th>Monthly administration</th>
      <th>Begin date</th>
      <th>End date</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Leasing</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenants.each do |tenant| %>
      <tr>
        <td><%= tenant.property %></td>
        <td><%= tenant.monthly_rent %></td>
        <td><%= tenant.monthly_administration %></td>
        <td><%= tenant.begin_date %></td>
        <td><%= tenant.end_date %></td>
        <td><%= tenant.name %></td>
        <td><%= tenant.phone %></td>
        <td><%= tenant.email %></td>
        <td><%= tenant.leasing %></td>
        <td><%= tenant.user %></td>
        <td><%= link_to 'Show', tenant %></td>
        <td><%= link_to 'Edit', edit_tenant_path(tenant) %></td>
        <td><%= link_to 'Destroy', tenant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenant', new_tenant_path %>
